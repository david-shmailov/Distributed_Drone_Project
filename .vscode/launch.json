    "version": "0.2.0",
    "configurations": [        
      {
        "name": "Launch GUI PC",
        "type": "erlang",
        "request": "launch",
        "cwd": "${workspaceRoot}/gui_pc/",
        "arguments": "-name gui@172.25.1.176 -setcookie cookie -eval \"application:start(gui_pc)\"",
        "preLaunchTask": "rebar3 compile gui_pc",
        // "runinterminal": ["cd gui_pc && rebar3", "shell"]
      },
      {
        "name": "Launch GS1 PC",
